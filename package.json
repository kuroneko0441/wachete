{
  "name": "wachete",
  "version": "2.0.0",
  "main": "./dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "test": "jest --coverage",
    "prebuild": "rimraf ./dist/",
    "build": "npm-run-all --parallel check-typings compile",
    "check-typings": "tsc -p tsconfig.app.json",
    "compile": "babel ./src/ --extensions .ts --ignore \"src/**/*.spec.ts\" --no-comments --out-dir ./dist/"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "jsdom": "^19.0.0",
    "mongodb": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.21",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  }
}
